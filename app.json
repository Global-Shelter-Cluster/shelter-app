{
  "expo": {
    "name": "Shelter Cluster",
    "description": "Mobile app for the Shelter Cluster",
    "slug": "sheltercluster",
    "version": "0.1.3",
    "sdkVersion": "28.0.0",
    "githubUrl": "https://github.com/Global-Shelter-Cluster/shelter-app",
